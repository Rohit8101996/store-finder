{
    "StartAt": "FindGeoCode",
    "States": {
      "FindGeoCode": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-1:dummy-account-number:function:atm-finder-dev-findGeoCode",
        "Next" : "ChoiceState"    
      },
      "ChoiceState" : {
        "Type" : "Choice",
        "Choices" : [
            {
                "Variable" : "$.searchable",
                "BooleanEquals" : true,
                "Next" : "PushToAlgolia"
            },
            {
                "Variable" : "$.searchable",
                "BooleanEquals" : false,
                "Next" : "SendToSlack"
            }
        ]
      },
      "PushToAlgolia" : {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-1:dummy-account-number:function:atm-finder-dev-pushtoalgolia",
        "Next" : "SendToSlack"
      },
      "SendToSlack" : {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-1:dummy-account-number:function:atm-finder-dev-sendtoslack",
        "End" : true
      }
    }
}